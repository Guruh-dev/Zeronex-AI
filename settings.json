{
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editorAssociations": {
        "*.pages": "default",
        "*.zip": "default"
    },
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "explorer.confirmDelete": false,
    "php.debug.executablePath": "index.php",
    "database-client.autoSync": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "GitHub Dark High Contrast",
    "[python]": {
        
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.defaultColorDecorators": "never",
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ],
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": "off"
    },
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        {
            "column": 80,
            "color": "#00FF0010"
        },
        {
            "column": 100,
            "color": "#BDB76B15"
        },
        {
            "column": 120,
            "color": "#FA807219"
        }
    ],
    "editor.unicodeHighlight.includeComments": true,
    "workbench.colorCustomizations": {
        "[Default Dark Modern]": {
            "tab.activeBorderTop": "#00FF00",
            "tab.unfocusedActiveBorderTop": "#00FF0088",
            "textCodeBlock.background": "#00000055"
        },
        "editor.wordHighlightStrongBorder": "#FF6347",
        "editor.wordHighlightBorder": "#FFD700",
        "editor.selectionHighlightBorder": "#A9A9A9"
    },
    "workbench.editor.revealIfOpen": true,
    "files.eol": "\n",
    "[bat]": {
        "files.eol": "\r\n"
    },
    "emmet.variables": {
        "lang": "en"
    },
    "cSpell.diagnosticLevel": "Hint",
    "trailing-spaces.backgroundColor": "rgba(255,0,0,0.1)",
    "trailing-spaces.includeEmptyLines": false,
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.detectJdksAtStart": false,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.debug.settings.hotCodeReplace": "auto",
    "java.dependency.packagePresentation": "hierarchical",
    "java.maxConcurrentBuilds": 16,
    "java.sources.organizeImports.staticStarThreshold": 1,
    "terminal.integrated.automationProfile.windows": {
        "path": "cmd"
    },
    "java.test.config": {
        "vmArgs": [
            "-Dstdout.encoding=UTF-8",
            "-Dstderr.encoding=UTF-8"
        ]
    },
    "maven.executable.path": "C:\\Users\\ASUS TUF GAMING\\AppData\\Roaming\\Code\\User\\globalStorage\\pleiades.java-extension-pack-jdk\\maven\\latest\\bin\\mvn"
}